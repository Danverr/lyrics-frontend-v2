{
	"name": "lyrics-frontend-v2",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"codegen": "sta -p https://api.lyrics.sslane.ru/openapi.json -t src/lib/api/templates -o src/lib/api --axios --unwrap-response-data --name api"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.189",
